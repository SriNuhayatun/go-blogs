@model BlogDashboard

    <h3>Data User</h3>

<table class="table">
    <thead>
        <tr>
            <th>
                Username
            </th>
            <th>
                Password
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.user)
        {
            <tr>
                <td>
                    @item.Username
                </td>
                <td>
                    @item.Password
                </td>

                <td>
                    <a asp-action="Ubah" asp-route-id="@item.Username">Edit</a>
                    <a asp-action="Details" asp-route-id="@item.Username">Details</a>
                    <a asp-action="Delete" asp-route-id="@item.Username">Delete</a>
                </td>
            </tr>

        }
    </tbody>


</table>
